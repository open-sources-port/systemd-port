{
  "version": 4,
  "configurations": [
    {
      "name": "macos-systemd",
      "includePath": [
        "${workspaceFolder}/macos-homebrew/include",
        "${workspaceFolder}/macos-homebrew/include/shared",
        "${workspaceFolder}/macos-homebrew/include/systemd",
        "${workspaceFolder}/macos-homebrew/include/basic",
        "${workspaceFolder}/macos-homebrew/include/fundamental",
        "/opt/homebrew/include",
        "/Library/Developer/CommandLineTools/usr/include"
      ],
      "defines": [
        "_DEBUG",
        "UNICODE",
        "_UNICODE"
      ],
      "compilerPath": "/usr/bin/clang",
      "cStandard": "c11",
      "cppStandard": "c++17",
      "intelliSenseMode": "macos-clang-x64",
      "browse": {
        "path": [
          "${workspaceFolder}/include",
          "${workspaceFolder}/macos-homebrew/src/libsystemd",
          "${workspaceFolder}/macos-homebrew/src/libudev"
        ],
        "limitSymbolsToIncludedHeaders": true
      },
      "macFrameworkPath": [
        "/System/Library/Frameworks",
        "/Library/Frameworks"
      ]
    },
    {
      "name": "windows-systemd",
      "includePath": [
        "${workspaceFolder}/include",
        "${workspaceFolder}/include/shared",
        "${workspaceFolder}/include/systemd",
        "C:/Program Files (x86)/Windows Kits/10/Include/10.0.19041.0/ucrt",
        "C:/Program Files (x86)/Windows Kits/10/Include/10.0.19041.0/shared",
        "C:/Program Files (x86)/Windows Kits/10/Include/10.0.19041.0/um"
      ],
      "defines": [
        "_DEBUG",
        "UNICODE",
        "_UNICODE",
        "_WINDOWS"
      ],
      "compilerPath": "C:/Program Files (x86)/Microsoft Visual Studio/2022/Community/VC/Tools/MSVC/14.35.32215/bin/Hostx64/x64/cl.exe",
      "cStandard": "c11",
      "cppStandard": "c++17",
      "intelliSenseMode": "windows-msvc-x64",
      "browse": {
        "path": [
          "${workspaceFolder}/include",
          "${workspaceFolder}/windows-cygwin/src"
        ],
        "limitSymbolsToIncludedHeaders": true
      }
    }
  ]
}
